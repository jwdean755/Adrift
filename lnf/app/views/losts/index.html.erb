<h1>Listing losts</h1>

<table>
  <tr>
    <th>catagory</th>
    <th>location</th>
    <th>description</th>
    <th>email</th>
  </tr>

  <% @losts.each do |lost| %>
    <tr>
      <td><%= lost.catagory %></td>
      <td><%= lost.location %></td>
      <td><%= lost.description %></td>
      <td><%= lost.email %></td>
    </tr>
  <% end %>
</table>
